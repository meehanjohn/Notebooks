      ]
     },
     "execution_count": 49,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "from array import array \n",
    "# Import the array type\n",
    "\n",
    "from random import random \n",
    "floats = array('d', (random() for i in range(10**7))) \n",
    "# Create an array of double-precision floats from any iterable object\n",
    "\n",
    "floats[-1]\n",
